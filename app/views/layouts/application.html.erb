<!DOCTYPE html>
<html>
  <head>
    <title>MidGreen</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.tree = "<%= image_url('tree.png') %>"
      window.solar = "<%= image_url('solar.png') %>"
